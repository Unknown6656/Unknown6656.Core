<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
    <PropertyGroup>
        <TargetFramework>net6.0-windows</TargetFramework>
        <EnablePreviewFeatures>True</EnablePreviewFeatures>
        <LangVersion>preview</LangVersion>
        <PlatformTarget>x64</PlatformTarget>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <OutputPath>$(SolutionDir)tests</OutputPath>
        <AssemblyName>Tester</AssemblyName>
        <ApplicationIcon />
        <OutputType>Exe</OutputType>
        <Nullable>enable</Nullable>
        <NullableContextOptions>enable</NullableContextOptions>
        <UseWPF>true</UseWPF>
        <UseWindowsForms>true</UseWindowsForms>
        <DisableWinExeOutputInference>true</DisableWinExeOutputInference>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.Net.Compilers.Toolset" Version="4.1.0">
          <PrivateAssets>all</PrivateAssets>
          <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
        <PackageReference Include="Unknown6656.DiscriminatedUnions" Version="1.0.1" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\..\Unknown6656.Core.Windows\Unknown6656.Core.Windows.csproj" />
        <ProjectReference Include="..\..\Unknown6656.Core\Unknown6656.Core.csproj" />
    </ItemGroup>
    <ItemGroup>
        <Folder Include="Properties\" />
    </ItemGroup>
    <ItemGroup>
        <None Update="img1.png">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </None>
        <None Update="img2-128.png">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </None>
        <None Update="img2-256.png">
            <CopyToOutputDirectory>Always</CopyToOutputDirectory>
        </None>
