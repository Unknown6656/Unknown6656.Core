    @"public static $type Parse(string str) => TryParse(str, out $type color) ? color : throw new ArgumentException($""The given string '{str}' cannot be parsed as a {typeof($type)}."", nameof(str));",
    @"public static bool TryParse(string str, out $type color)
    {
        static string normalize(string input) => input.ToLowerInvariant().Remove('-', '_', ' ', '\r', '\n', '\v', '\t', '\0', '\b', '\a');

        color = default;
        str = normalize(str);

        if (str.Match(RGBAColor.REGEX_HEX, out Match? match) && uint.TryParse(match.Groups[""hex""].Value, NumberStyles.HexNumber, null, out uint hex))
        {
            color = new(hex);

            return true;
        }
        if (str.Match(RGBAColor.REGEX_CSS, out match))
        {
            static double parse(string value)
            {
                if (value.Length is 0)
                    return 0;
                else if (value[^1] == '%' && double.TryParse(value[..^1], out double d))
                    return d * .01;
                else if (double.TryParse(value, out d))
                {
                    if (value.Contains('.') && Math.Abs(d) <= 1)
                        d /= 255;

                    return d;
                }
                else
                    return 0;
            }
            string mode = match.Groups[""mode""].Value;
            double x = parse(match.Groups[""x""].Value);
            double y = parse(match.Groups[""y""].Value);
            double z = parse(match.Groups[""z""].Value);
            double w = parse(match.Groups[""w""].Value);

            if (mode is ""rgb"")
                color = new(x, y, z);
            else if (mode is ""rgba"")
                color = new(x, y, z, w);
            else if (mode is ""hsv"")
                color = FromHSV(x, y, z);
            else if (mode is ""hsva"")
                color = FromHSV(x, y, z, w);
            else if (mode is ""hsl"")
                color = FromHSL(x, y, z);
            else if (mode is ""hsla"")
                color = FromHSL(x, y, z, w);
            else if (mode is ""hcl"")
                color = FromHCL(x, y, z);
            else if (mode is ""hcla"")
                color = FromHCL(x, y, z, w);
            else if (mode is ""hcy"")
                color = FromHCY(x, y, z);
            else if (mode is ""hcya"")
                color = FromHCY(x, y, z, w);
            else if (mode is ""xyz"")
                color = FromXYZ(x, y, z);
            else if (mode is ""yuv"")
                color = FromYUV(x, y, z);
            else if (mode is ""uv"")
                color = FromUV(x, y);
            else if (mode is ""xyza"")
                color = FromXYZ(x, y, z, w);
            else if (mode is ""yuva"")
                color = FromYUV(x, y, z, w);
            else if (mode is ""uva"")
                color = FromUV(x, y, w);


            // TODO: yiq
            // TODO: ycbcr / ybr
            // TODO: cmyk

            else
                return false;

            return true;
        }
        else if (IColor.KnownColorNames.FirstOrDefault(kvp => normalize(kvp.Value) == str) is { Key: uint hex2, Value: { } })
        {
            color = new(hex2);

            return true;
        }
        else
            return false;
    }",
