{
    var duplicates = colors.Where(t => t.hex.Equals(hex, StringComparison.InvariantCultureIgnoreCase)).Select(t => t.name).ToArray();

    if (duplicates.Length == 0)
        colors.Add((hex, name));
    else
        // && !t.Item2.Equals(name, StringComparison.InvariantCultureIgnoreCase)
        dup_colors.Add((hex, name, duplicates));
}

